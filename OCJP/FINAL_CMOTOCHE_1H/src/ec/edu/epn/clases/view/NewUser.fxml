<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.epn.clases.controller.NewUserController">
    <stylesheets>
        <URL value="@DarkTheme.css" />
    </stylesheets>
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Register New User" textAlignment="CENTER" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="86.0" />
        <TextField fx:id="txtUserName" layoutX="65.0" layoutY="67.0" promptText="User Name" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
        <TextField fx:id="txtPassword" layoutX="65.0" layoutY="111.0" promptText="Password" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
        <ButtonBar layoutX="87.0" layoutY="147.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
            <buttons>
                <Button fx:id="btnAcept" mnemonicParsing="false" onAction="#handleAcept" text="Acept" />
                <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
